{
  "name": "specializer",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "typescript": "^5.4.5",
    "wireit": "^0.14.12"
  },
  "scripts": {
    "build": "wireit"
  },
  "wireit": {
    "build": {
      "command": "tsc",
      "files": [
        "src/**/*.ts",
        "tsconfig.json"
      ],
      "output": [
        "dist/**"
      ],
      "clean": "if-file-deleted"
    }
  }
}